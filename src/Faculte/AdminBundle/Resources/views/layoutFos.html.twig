<body class=" login">
<!-- BEGIN LOGO -->
<div class="logo">

</div>
<!-- END LOGO -->
<!-- BEGIN LOGIN -->
<div class="content">
    <!-- BEGIN LOGIN FORM -->
    <form class="login-form" action="{{ path("fos_user_security_check") }}" method="post">
        {% if csrf_token %}
            <div> <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" /></div>
        {% endif %}
        <h3 class="form-title" style="color: black"> Bienvenus</h3>
        <div class="alert alert-danger display-hide">
            <button class="close" data-close="alert"></button>
            <span> Enter any username and password. </span>
        </div>
        <div class="login-content">
            {% if error %}
                <div class="alert alert-danger" role="alert">

                    <button class="close" data-close="alert"></button>
                    <span>{{ error.messageKey|trans(error.messageData, 'security') }}</span>
                </div>
            {% endif %}
        </div>
        <div class="form-group">
            <!--ie8, ie9 does not support html5 placeholder, so we just show field title for that-->
            <label class="control-label visible-ie8 visible-ie9">Username</label>
            <input class="form-control placeholder-no-fix" type="text" autocomplete="off" placeholder="login"  id="username" name="_username" value="{{ last_username }}" required="required"  /> </div>
        <div class="form-group">
            <label class="control-label visible-ie8 visible-ie9">Password</label>
            <input class="form-control form-control-solid placeholder-no-fix" type="password" autocomplete="off" placeholder="Mot de passe" id="password" name="_password" required="required"  required /> </div>
        <div class="form-actions">
            <button type="submit" class="btn btn-success uppercase ">Connexion</button><br>
            <a href="javascript:;"id="forget-password" class="forget-password">Vous avez oublié votre mot de passe ? </a><br>
        </div>
        <div class="create-account">
        </div>
    </form>
    <!-- END LOGIN FORM -->
    <!-- BEGIN FORGOT PASSWORD FORM -->
    {% trans_default_domain 'FOSUserBundle' %}
    <form class="forget-form" action="{{ path('fos_user_resetting_send_email') }}" method="post" class="fos_user_resetting_request">
        <h3 style="color: black">Vous avez oublié votre mot de passe ?</h3>
        <p>Entrez votre adresse e-mail ci-dessous pour réinitialiser votre mot de passe. </p>
        <div class="form-group">
            <label for="username">{{ 'resetting.request.username'|trans }}</label>
            <input class="form-control placeholder-no-fix" type="text" autocomplete="off" placeholder="Email" id="username" name="username" required="required" /> </div>
        <div class="form-actions">
            <button type="button" id="back-btn" class="btn btn-default">Retour</button>
            <button type="submit"  value="{{ 'resetting.request.submit'|trans }}"class="btn btn-success uppercase pull-right">Envoyer</button>
        </div>
    </form>
    <!-- END FORGOT PASSWORD FORM -->
</div>
